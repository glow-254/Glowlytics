{
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "framework": "nextjs",
    "outputDirectory": "frontend/.next",
    "rootDirectory": "frontend",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}